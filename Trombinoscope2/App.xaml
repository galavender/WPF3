<Application x:Class="Trombinoscope.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Trombinoscope"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>

            <DataTemplate DataType="{x:Type local:VMEmployes}">
                <local:UCEmployes/>
            </DataTemplate>

            <DataTemplate DataType="{x:Type local:VMTrombi}">
                <local:UCTrombi/>
            </DataTemplate>

            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="DicoBrosse.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>



    </Application.Resources>
</Application>
